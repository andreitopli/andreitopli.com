generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  title       String
  description String
  featured    String
  id          Int    @id(map: "portfolio_pkey") @unique(map: "portfolio_id_key") @default(autoincrement())
  url         String
}
