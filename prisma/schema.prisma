generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["dataProxy"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  title       String  @unique
  description String
  featured    String
  id          Int     @id @default(autoincrement())
  url         String?
}
